                        <div class="tab-pane fade" id="otro" role="tabpanel" aria-labelledby="otro-tab">
                            <form id="formOtros">
                                <fieldset id="paso1" style="display: block;">
                                    <h5 class="mt-3">Funcionamiento:</h5>
                                    <div class="p-3 shadow-sm mb-3">
                                        <div class="row">
                                            <div class="form-group col-sm-6">
                                                <label for="turnoFormControlSelect">Turno de funcionamiento</label>
                                                <select class="form-control form-select form-select-sm" id="turnoFormControlSelect" name="turnoFormControlSelect" aria-label=".form-select-sm example">
                                                    <option value="todo" selected>Sin seleccionar</option>
                                                    <option value="manana">Mañana</option>
                                                    <option value="tarde">Tarde</option>
                                                    <option value="vespertino">Noche</option>
                                                    <option value="mañana extendida">Mañana extendida</option>
                                                    <option value="tarde extendida">Tarde extendida</option>
                                                    <option value="ninguno">Ninguno de los anteriores</option>
                                                </select>
                                            </div>
                                            <div class="form-group col-sm-6">
                                                <label for="jornadaFormControlSelect">Jornada extendida/completa</label>
                                                <select class="form-control form-select form-select-sm" id="jornadaFormControlSelect" name="jornadaFormControlSelect"  aria-label=".form-select-sm example">
                                                    <option value="todo" selected>Sin seleccionar</option>
